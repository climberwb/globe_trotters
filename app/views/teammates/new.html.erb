<h1>Teammates#new</h1>
<p>Find me in app/views/teammates/new.html.erb</p>
<h1>Make a New Team</h1>
 <div class= "row">
   <div class = "col-md-4">
     <p>Guidlines for wikis</p>
     <ul>
       <li>Keep it clean</li>
       <li>Keep it safe</li>
       <li>Keep it classy Sandiego </li>
     </ul>
   </div>
 </div>
<div class = "col-md-8">
  <%= form_for  :teammate, url: teammates_path(@teammate), method: :post  do |f|
 %><%= current_user %>
    <div class = "form-group">
      <%= f.label :bio %>
      <%= f.text_area :bio, class: 'form-control', placeholder: "Enter wiki title", id:'team_title' %>
    </div>
    <div class = "form-group">
      <%= f.submit class: 'btn btn-success' ,id: 'team_role_submit'  %>
    </div>
  <% end %>
  </div>

</div>